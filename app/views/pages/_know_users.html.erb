<ul>
  <% unless @know_users.empty? %>
    <% @know_users.each do |user| %>
      <%= render "pages/user", user: user %>
    <% end %>
    <div class="users-load-more">
      <%= link_to "もっと見る", follow_users_path(type: "know_users") %>
    </div>
  <% else %>
    <p>
      <%= t :no_content %>
    </p>
  <% end %>
</ul>
