<div class="user-infomation">
  <div class="info">
    <%= label_tag "メイル: " %>
    <%= @user.email %>
  </div>

  <div class="info">
    <%= label_tag "電話番号: " %>
    <%= @user.profile.phone_number %>
  </div>

  <div class="info">
    <%= label_tag "出身所: " %>
    <%= @user.profile.place_birth %>
  </div>

  <div class="info">
    <%= label_tag "誕生日: " %>
    <%= @user.profile.birthday %>
  </div>
  <% if current_user.current_user? @user %>
    <button class="btn btn-primary user-edit-btn">
      編集
    </button>
  <% end %>
  <div class="clearfix"></div>
</div>
